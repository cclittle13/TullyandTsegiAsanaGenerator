+def load_users():
+    """
+    load fake users from user.csv into database
+    """
+
+    #yoga_users is a csv file 
+    users_file = open("yoga_users")
+
+    for line in users_file:
+        line = line.rstrip()
+        line = line.split(",")
+        a_user = User(email=line[0],
+                      password=line[1])
+        db.session.add(a_user)
+
+    db.session.commit()
+
