+    // Set these variables to their DOM elements
+    var myImage = document.getElementById("coffee-image");
+    var myRevenueMessage = document.getElementById("message");
+    myImage.setAttribute('src', '');
+    function coffeeMachine() {
+        myImage.setAttribute('src', 'cat_balance.jpg');
+    }
+    function coffeeBeans() {
+        myImage.setAttribute('src', 'downward_dog_leg_up_kneeling.jpg');
+    }
+    function coffeeMug() {
+        myImage.setAttribute('src', 'supine_bound_angle.jpg');
+    }
+    function makeCoffee() {
+        coffeeMachine();
+        setTimeout(coffeeMug, 1000);
+        setTimeout(coffeeBeans, 2500);
+    }
+    myImage.addEventListener('click', makeCoffee);
+    // use this JavaScript object in further study step
+    var prices = {
+        "brew": 2.05,
+        "espresso": 3.25,
