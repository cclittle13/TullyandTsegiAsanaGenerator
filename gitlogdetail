+def load_poses():
+    """
+    load existing users from user.csv into database
+    """
+
+    print "Poses"
+
+    #yoga_users is a csv file 
+    users_file = open("poses_yoga.csv")
+
+    for line in users_file:
+        line = line.rstrip()
+        line = line.split(",")
+        pose = Pose(pose=line[0],
+                      sanskrit=line[1])
+        db.session.add(pose)
+
+    # user = User(user_id=user_id, email=email)
+
+    db.session.commit()
